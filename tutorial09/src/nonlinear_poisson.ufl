cell = triangle
degree = 1
V = FiniteElement("CG", cell, degree)
u = TrialFunction(V)
v = TestFunction(V)

x = SpatialCoordinate(cell)
dim = 2
eta = 2
g = x[0]*x[0] + x[1]*x[1]
f = - 2*dim + eta*g*g

def q(u):
    return eta*u*u

r = (inner(grad(u), grad(v)) + q(u)*v - f*v) *dx

exact_solution = g
is_dirichlet = 1
interpolate_expression = g
